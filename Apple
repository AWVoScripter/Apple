local function FUZVK_fake_script() 
	local script = Instance.new('LocalScript', execute)

	local Execute = script.Parent.Parent.execute
	local Editor = script.Parent.Parent.Editor
	local skidlol = script.Parent.Parent.Skid
	
	
	script.Parent.Parent.execute.MouseButton1Click:Connect(function()
		loadstring(Editor.Text)
	end)
end
coroutine.wrap(FUZVK_fake_script)()
local function DXTLXG_fake_script() 
	local script = Instance.new('LocalScript', Clear)

	local Editor = script.Parent.Parent.Editor
	local Clear = script.Parent.Parent.Clear
	
	script.Parent.Parent.Clear.MouseButton1Click:Connect(function()
		Editor.Text = "Cleared!"
		wait(0.6)
		Editor.Text = ""
	end)
	
end
coroutine.wrap(DXTLXG_fake_script)()
local function SPVTZY_fake_script() 
	local script = Instance.new('LocalScript', Inject)

	local injectbutton = script.Parent.Parent.Inject
	local executebutton = script.Parent.Parent.execute
	local clearbutton = script.Parent.Parent.Clear
	local iy = script.Parent.Parent.IY
	
	local function inject ()
		injectbutton.Text = "Injecting."
		wait(0.4)
		injectbutton.Text = "Injecting.."
		wait(0.4)
		injectbutton.Text = "Injecting..."
		wait(0.4)
		injectbutton.Text = "Injecting."
		wait(0.4)
		injectbutton.Text = "Injected!"
		wait(0.2)
		
		injectbutton.Visible = false
		executebutton.Visible = true
		clearbutton.Visible = true
		iy.Visible = true
		print("Apple was injected, have fun scripting!")
	end
	
	injectbutton.MouseButton1Click:Connect(function()
		inject()
	end)
end
coroutine.wrap(SPVTZY_fake_script)()
local function JAMIFI_fake_script() 
	local script = Instance.new('LocalScript', IY)

	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
	end)
	
end
coroutine.wrap(JAMIFI_fake_script)()
